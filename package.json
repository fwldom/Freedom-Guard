{
    "name": "freedom-guard",
    "productName": "Freedom Guard",
    "description": "Warp + Psiphon + Freedom Vibe + Freedom Get",
    "keywords": [],
    "main": "./main.js",
    "version": "1.2.7",
    "author": "fwldom <fwldom@duck.com>",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder"
    },
    "build": {
        "extraResources": [
            {
                "from": "assets/",
                "to": "assets/",
                "filter": [
                    "**/*"
                ]
            },
            {
                "from": "svgs/",
                "to": "svgs/",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "productName": "Freedom Guard",
        "appId": "guard.freedom.com",
        "artifactName": "freedom-guard-${os}-${arch}.${ext}",
        "asar": true,
        "publish": null,
        "directories": {
            "output": "dist"
        },
        "win": {
            "icon": "ico.ico",
            "target": "nsis"
        },
        "linux": {
            "target": "tar.gz",
            "category": "VPN",
            "icon": "ico.png"
        },
        "files": [
            "assets/**/*",
            "node_modules/boxicons/css/boxicons.min.css",
            "node_modules/**/*",
            "package.json",
            "main.js",
            "rederer.js",
            "preload.js",
            "*.js",
            "*.html",
            "*.css",
            "fonts/*.*",
            "svgs/*.*",
            "*.png",
            "*.ico"
        ]
    },
    "dependencies": {
        "axios": "^1.7.2",
        "geoip-lite": "^1.4.10"
    },
    "devDependencies": {
        "electron": "31.1.0",
        "fs": "^0.0.1-security"
    },
    "bugs": {
        "url": "https://github.com/fwldom/Freedom-Guard-Electron/issues",
        "email": "fwldom@duck.com"
    }
}
